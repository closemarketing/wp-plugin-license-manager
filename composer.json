{
	"name": "closemarketing/wp-plugin-license-manager",
	"description": "WordPress Plugin License Manager Library - A reusable library for managing WordPress plugin licenses with Enwikuna License Manager",
	"type": "library",
	"keywords": [
		"wordpress",
		"license",
		"plugin",
		"enwikuna",
		"license-manager",
		"updates"
	],
	"license": "GPL-2.0-or-later",
	"authors": [
		{
			"name": "David Perez",
			"email": "david@closemarketing.es",
			"homepage": "https://close.marketing",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=7.4"
	},
	"require-dev": {
		"phpstan/phpstan": "^1.10",
		"wp-coding-standards/wpcs": "^3.0",
		"phpcompatibility/phpcompatibility-wp": "*"
	},
	"autoload": {
		"psr-4": {
			"Closemarketing\\WPLicenseManager\\": "src/"
		},
		"files": []
	},
	"minimum-stability": "stable",
	"prefer-stable": true,
	"support": {
		"issues": "https://github.com/closemarketing/wp-plugin-license-manager/issues",
		"source": "https://github.com/closemarketing/wp-plugin-license-manager"
	}
}

